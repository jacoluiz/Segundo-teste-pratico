					"listen": "test",
					"script": {
						"exec": [
							"var resposta = xml2Json(responseBody);\r",
							"resposta = resposta['soap:Envelope']['soap:Body']['ns2:consultaCEPResponse']['return'];\r",
							"\r",
							"pm.test(\"Status code retornado foi 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Retornou bairro\", function () {\r",
							"    pm.expect(resposta.bairro).to.not.empty;\r",
							"});\r",
							"\r",
							"pm.test(\"Retornou CEP\", function () {\r",
							"    pm.expect(resposta.cep).to.not.empty;\r",
							"});\r",
							"\r",
							"pm.test(\"Retornou cidade\", function () {\r",
							"    pm.expect(resposta.cidade).to.not.empty;\r",
							"});\r",
							"\r",
							"pm.test(\"Retornou complemento\", function () {\r",
							"    pm.expect(resposta.complemento2).to.not.empty;\r",
							"});\r",
							"\r",
							"pm.test(\"Retornou endereço\", function () {\r",
							"    pm.expect(resposta.end).to.not.empty;\r",
							"});\r",
							"\r",
							"pm.test(\"Retornou UF\", function () {\r",
							"   pm.expect(resposta.uf).to.not.empty;\r",
							"});\r",
							""
